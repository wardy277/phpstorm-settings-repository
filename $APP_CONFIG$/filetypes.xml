<application>
  <component name="FileTypeManager" version="11">
    <ignoreFiles list="CVS;SCCS;RCS;rcs;.DS_Store;.svn;.pyc;.pyo;*.pyc;*.pyo;.git;*.hprof;_svn;.hg;*.lib;*~;__pycache__;.bundle;*.rbc;" />
    <extensionMap>
      <removed_mapping ext="html" type="HTML" />
      <mapping ext="html" type="PHP" />
      <mapping pattern=".htacccess" type="PLAIN_TEXT" />
      <mapping pattern=".htpasswd" type="PLAIN_TEXT" />
    </extensionMap>
    <setting name="fileTypeChangedCounter" value="4" />
  </component>
</application>